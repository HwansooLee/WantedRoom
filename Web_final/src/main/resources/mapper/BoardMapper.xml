<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.human.dao.IF_BoardDAO">
    <insert id="insert" parameterType="com.human.VO.BoardVO">
        insert into board values (boardNoSeq.nextval, #{id}, #{itemNo}, #{title},
                                #{content}, #{inDate}, #{views} )
    </insert>
    <select id="selectAll" parameterType="int" resultType="com.human.VO.BoardVO">
        select * from board where boardNo=#{boardNo}
    </select>


</mapper>